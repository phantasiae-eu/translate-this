{
  "expo": {
    "name": "Trathi",
    "slug": "trathi",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/trathi-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7B87D5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "eu.phantasiae.translatethis",
      "icon": "./assets/trathi-ios.png"
    },
    "android": {
      "splash": {
        "image": "./assets/trathi-splash.png",
        "resizeMode": "native",
        "backgroundColor": "#7B87D5"
      },
      "package": "eu.phantasiae.translatethis",
      "icon": "./assets/trathi-android.png"
    }
  }
}